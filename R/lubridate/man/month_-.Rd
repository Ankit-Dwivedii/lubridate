function (x, value) 
{
    if (value - trunc(value) != 0) 
        stop("Month must be an integer")
    ISOdatetime(year(x) + (value - 1)\%/\%12, (value - 1)\%\%12 + 
        1, mday(x), hour(x), minute(x), second(x), tz(x))
  }
}
